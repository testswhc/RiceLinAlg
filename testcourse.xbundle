        <vertical display_name="Row-Reduced Echelon Form" refnum="2" url_name="b1rowreduced-vertical2">
          <video display_name="RREF" youtube_id_1_0="Qi3O6iLhZ0E" url_name="b1rowreduced-tab2-video1"/>
