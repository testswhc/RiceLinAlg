      <sequential display_name="Linear Combinations and Span" linenum="180" tex_filename="testcourse.tex" refnum="4" due="2018-12-18T14:15" graded="true" format="Exercises" url_name="b1lincombos-sequential">
        <vertical display_name="Page One" refnum="1" url_name="b1lincombos-vertical1">
